{
  "name": "Toshocat",
  "version": "0.0.1",
  "description": "Anime and Manga tracking application for Desktop",
  "main": "main.js",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development ./node_modules/.bin/electron main/index --enable-logging",
    "start": "cross-env NODE_ENV=production ./node_modules/.bin/electron start.js",
    "webpack:dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "test": "mocha --compilers js:babel-core/register --recursive --timeout 2000 --require mock-local-storage",
    "test:watch": "npm run test --watch",
    "coverage": "nyc --require babel-core/register mocha ./test --recursive --compilers js:babel-core/register --timeout 2000 --require mock-local-storage",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "webpack:build": "cross-env NODE_ENV=production webpack -p",
    "lint": "eslint -c .eslintrc src/**/*.js",
    "rebuild": "./node_modules/.bin/electron-rebuild.cmd",
    "pack": "build",
    "dist": "build"
  },
  "author": "Dennis Jin <dennis@tofuness.com>",
  "license": "GPL-3.0",
  "build": {
    "asar": true,
    "ignore": "src",
    "msi": true,
    "productName": "Toshocat",
    "compression": "store",
    "win": {
      "iconUrl": "https://raw.githubusercontent.com/tofuness/Toshocat/master/app-icon.ico"
    }
  },
  "dependencies": {
    "anitomy-js": "^1.0.5",
    "cartesian-product": "^2.1.2",
    "classnames": "^2.2.3",
    "configstore": "^2.0.0",
    "cross-env": "^1.0.7",
    "electron-positioner": "git+https://github.com/tofuness/electron-positioner.git",
    "electron-prebuilt": "^1.0.2",
    "electron-window": "^0.8.1",
    "execa": "^0.4.0",
    "history": "^2.1.1",
    "lodash": "^4.12.0",
    "mapreplace": "^1.0.3",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "node-uuid": "^1.4.7",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-autosize-textarea": "^0.3.0",
    "react-collapse": "^2.2.0",
    "react-dom": "^15.0.2",
    "react-height": "^2.1.0",
    "react-hotkey": "git+https://github.com/tofuness/react-hotkey.git",
    "react-list": "^0.7.18",
    "react-motion": "^0.4.3",
    "react-outside-event": "^1.2.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-tooltip": "^2.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.0.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "seamless-immutable": "^6.1.0",
    "spin.js": "^2.3.2",
    "superagent": "^1.8.3",
    "tcomb-form": "^0.9.5",
    "velocity-animate": "^1.2.3"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-istanbul": "^0.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.0.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "mock-local-storage": "^1.0.2",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "nyc": "^6.4.4",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.0.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tofuness/Toshocat.git"
  },
  "bugs": {
    "url": "https://github.com/tofuness/Toshocat/issues"
  },
  "homepage": "https://github.com/tofuness/Toshocat#readme"
}
